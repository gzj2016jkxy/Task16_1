<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.gzj.entity.Produce" table="produce">

		<id name="produceId" type="java.lang.Integer">
			<column name="produceId" />
			<generator class="native"></generator>
		</id>

		<property name="produceName" type="java.lang.String">
			<column name="produceName" length="40" />
		</property>		
				
	</class>

</hibernate-mapping> 
