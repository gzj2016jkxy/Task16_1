<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.gzj.entity.Goods" table="goods">

		<id name="goodsId" type="java.lang.Integer">
			<column name="goodsId" />
			<generator class="native"></generator>
		</id>

		<property name="goodsName" type="java.lang.String">
			<column name="goodsName" length="40" />
		</property>
		<property name="goodsmaterial" type="java.lang.String">
			<column name="goodsmaterial" length="200" />
		</property>
		<property name="goodsPackage" type="java.lang.String">
			<column name="goodsPackage" length="200" />
		</property>
		<property name="goodsLanguage" type="java.lang.String">
			<column name="goodsLanguage" length="200" />
		</property>
		<property name="goodsPrice" type="java.lang.Float">
			<column name="goodsPrice" />
		</property>
		<property name="goodsCent" type="java.lang.String">
			<column name="goodsCent" length="40" />
		</property>
		<property name="goodsScope" type="java.lang.String">
			<column name="goodsScope" length="100" />
		</property>
		<property name="goodsPlace" type="java.lang.String">
			<column name="goodsPlace" length="100" />
		</property>
		<property name="goodsHabitus" type="java.lang.String">
			<column name="goodsHabitus" length="100" />
		</property>
		<property name="sortName" type="java.lang.String">
			<column name="sortName" length="40" />
		</property>		
				
	</class>

</hibernate-mapping> 
