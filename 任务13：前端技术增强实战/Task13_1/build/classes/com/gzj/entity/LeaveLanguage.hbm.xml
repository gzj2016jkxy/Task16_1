<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.gzj.entity.LeaveLanguage" table="leaveLanguage">

		<id name="leaveLanguageId" type="java.lang.Integer">
			<column name="leaveLanguageId" />
			<generator class="native"></generator>
		</id>

		<property name="name" type="java.lang.String">
			<column name="name" length="40" />
		</property>		
		<property name="qq" type="java.lang.String">
			<column name="qq" length="40" />
		</property>		
		<property name="email" type="java.lang.String">
			<column name="email" length="100" />
		</property>		
		<property name="leaveLanguage" type="java.lang.String">
			<column name="leaveLanguage" length="200" />
		</property>		
		<property name="dateTime" type="java.lang.String">
			<column name="dateTime" length="40" />
		</property>		
				
	</class>

</hibernate-mapping> 
